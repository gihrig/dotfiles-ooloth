  "whichkey.bindings": [
    // see: https://vspacecode.github.io/docs/whichkey/reference
    /*
      Keyboard shortcut ideas (multiple times or very common; chords):
      - C-] = next editor/terminal
      - C-[ = previous editor/terminal
      - C-t = toggle view terminal
      - C-p = toggle view problems

      Vim ideas (multiple times or very common; sequences)
      - ] = next...
          - c = change/conflict (depending on if in editor vs diff vs merge conflict view)
      - [ = previous...
          - (same as above)

      WhichKey ideas (one shot or uncommon):
      - ] = next...
          - e = editor
          - g = editor group
          - p = problem in files
          - t = terminal
      - [ = previous...
          - (same as above)
      - a = view code actions (quick fixes)
      - a = actions...
          - a = actions for current line
          - n = next marker
          - p = previous marker
      - d = debugging...
      - e = errors/problems...
        - n = next (editor.action.marker.nextInFiles)
        - p = previous (editor.action.marker.previousInFiles)
      - f = finding/searching...
      - folding...
      - git (including gitlens, lazygit, etc.)
      - new...
        - file
        - terminal
      - o = open...
        - e = explorer
        - f = new file
        - t = terminal
        - x = extensions
      - p = problems...
        - a = all
        - n = next
        - p = previous
      - r = replace...
      - s = settings/ui...
        - l = line numbers (toggle)
        - w = word wrap (toggle)
      - v = view...
        - a = activity bar
        - b = sidebar
        - c = comments
        - d = debugger
        - e = explorer
        - f = find
        - F = find and replace
        - o = output
        - p = problems
        - P = panel
        - s = status bar
        - t = terminal
        - x = extensions
        - z = zen mode

    */
    { "key": " ", "name": "Commands", "command": "workbench.action.showCommands", "type": "command" },
    { "key": ".", "name": "Repeat most recent action", "command": "whichkey.repeatMostRecent", "type": "command" },
    { "key": "?", "name": "Search keybindings", "command": "whichkey.searchBindings", "type": "command" },
        { "key": "u", "name": "+UI", "type": "bindings", "bindings": [
      { "key": "a", "name": "Activity bar (toggle)", "command": "workbench.action.toggleActivityBarVisibility", "type": "command" },
      { "key": "b", "name": "Sidebar (toggle)", "command": "workbench.action.toggleSidebarVisibility", "type": "command" },
      { "key": "c", "name": "Color theme", "command": "workbench.action.selectTheme", "type": "command" },
      { "key": "f", "name": "Full screen (toggle)", "command": "workbench.action.toggleFullScreen", "type": "command" },
      { "key": "l", "name": "Line numbers (toggle)", "command": "settings.cycle.lineNumbers", "type": "command" },
      { "key": "r", "name": "Relative line numbers (toggle)", "command": "settings.cycle.relativeLineNumbers", "type": "command" },
      { "key": "m", "name": "Maximized panel (toggle)", "command": "workbench.action.toggleMaximizedPanel", "type": "command" },
      { "key": "p", "name": "Panel (toggle)", "command": "workbench.action.togglePanel", "type": "command" },
      { "key": "s", "name": "Statusbar (toggle)", "command": "workbench.action.toggleStatusbarVisibility", "type": "command" },
      { "key": "t", "name": "Tabs (toggle)", "command": "workbench.action.toggleTabsVisibility", "type": "command" },
      { "key": "w", "name": "Word wrap (toggle)", "command": "editor.action.toggleWordWrap", "type": "command" },
      { "key": "z", "name": "Zen mode (toggle)", "command": "workbench.action.toggleZenMode", "type": "command" }
    ]},

    { "key": "-", "name": "------------------", "command": " ", "type": "command" },

    // { "key": "\t", "name": "Last editor", "commands": [ "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup", "list.select" ], "type": "commands" },
    { "key": "b", "name": "+Buffers/Editors", "type": "bindings", "bindings": [
      { "key": "b", "name": "Show all buffers/editors", "command": "workbench.action.showAllEditors", "type": "command" },
      { "key": "B", "name": "Show all buffers/editors in active group", "command": "workbench.action.showEditorsInActiveGroup", "type": "command" },
      { "key": "d", "name": "Close active editor", "command": "workbench.action.closeActiveEditor", "type": "command" },
      { "key": "H", "name": "Move editor into left group", "command": "workbench.action.moveEditorToLeftGroup", "type": "command" },
      { "key": "J", "name": "Move editor into below group", "command": "workbench.action.moveEditorToBelowGroup", "type": "command" },
      { "key": "K", "name": "Move editor into above group", "command": "workbench.action.moveEditorToAboveGroup", "type": "command" },
      { "key": "L", "name": "Move editor into right group", "command": "workbench.action.moveEditorToRightGroup", "type": "command" },
      { "key": "M", "name": "Close other editors", "command": "workbench.action.closeOtherEditors", "type": "command" },
      { "key": "n", "name": "Next editor", "command": "workbench.action.nextEditor", "type": "command" },
      { "key": "p", "name": "Previous editor", "command": "workbench.action.previousEditor", "type": "command" },
      { "key": "N", "name": "New untitled editor", "command": "workbench.action.files.newUntitledFile", "type": "command" },
      { "key": "u", "name": "Reopen closed editor", "command": "workbench.action.reopenClosedEditor", "type": "command" },
      { "key": "P", "name": "Paste clipboard to buffer", "commands": [ "editor.action.selectAll", "editor.action.clipboardPasteAction" ], "type": "commands" },
      { "key": "Y", "name": "Copy buffer to clipboard", "commands": [ "editor.action.selectAll", "editor.action.clipboardCopyAction", "cancelSelection" ], "type": "commands" }
    ]},
    { "key": "d", "name": "+Debug", "type": "bindings", "bindings": [
      { "key": "d", "name": "Start debug", "command": "workbench.action.debug.start", "type": "command" },
      { "key": "D", "name": "Run without debugging", "command": "workbench.action.debug.run", "type": "command" },
      { "key": "S", "name": "Stop debug", "command": "workbench.action.debug.stop", "type": "command" },
      { "key": "c", "name": "Continue debug", "command": "workbench.action.debug.continue", "type": "command" },
      { "key": "p", "name": "Pause debug", "command": "workbench.action.debug.pause", "type": "command" },
      { "key": "R", "name": "Restart debug", "command": "workbench.action.debug.restart", "type": "command" },
      { "key": "i", "name": "Step into", "command": "workbench.action.debug.stepInto", "type": "command" },
      { "key": "s", "name": "Step over", "command": "workbench.action.debug.stepOver", "type": "command" },
      { "key": "o", "name": "Step out", "command": "workbench.action.debug.stepOut", "type": "command" },
      { "key": "b", "name": "Toggle breakpoint", "command": "editor.debug.action.toggleBreakpoint", "type": "command" },
      { "key": "B", "name": "Toggle inline breakpoint", "command": "editor.debug.action.toggleInlineBreakpoint", "type": "command" },
      { "key": "j", "name": "Jump to cursor", "command": "debug.jumpToCursor", "type": "command" },
      { "key": "v", "name": "REPL", "command": "workbench.debug.action.toggleRepl", "type": "command" },
      { "key": "w", "name": "Focus on watch window", "command": "workbench.debug.action.focusWatchView", "type": "command" },
      { "key": "W", "name": "Add to watch", "command": "editor.debug.action.selectionToWatch", "type": "command" }
    ]},
    { "key": "e", "name": "+Errors", "type": "bindings", "bindings": [
      { "key": ".", "name": "Error transient", "command": "whichkey.showTransient", "args": "whichkey.transient.error", "type": "command" },
      { "key": "l", "name": "List errors", "command": "workbench.actions.view.problems", "type": "command" },
      { "key": "n", "name": "Next error", "command": "editor.action.marker.next", "type": "command" },
      { "key": "p", "name": "Previous error", "command": "editor.action.marker.prev", "type": "command" }
    ]},
    { "key": "f", "name": "+File", "type": "bindings", "bindings": [
      { "key": "f", "name": "Open file/folder", "command": "whichkey.openFile", "type": "command" },
      { "key": "n", "name": "New Untitled", "command": "workbench.action.files.newUntitledFile", "type": "command" },
      { "key": "w", "name": "Open active in new window", "command": "workbench.action.files.showOpenedFileInNewWindow", "type": "command" },
      { "key": "s", "name": "Save file", "command": "workbench.action.files.save", "type": "command" },
      { "key": "S", "name": "Save all files", "command": "workbench.action.files.saveAll", "type": "command" },
      { "key": "r", "name": "Open recent", "command": "workbench.action.openRecent", "type": "command" },
      { "key": "R", "name": "Rename file", "commands": [ "workbench.files.action.showActiveFileInExplorer", "renameFile" ], "type": "commands" },
      { "key": "t", "name": "Show tree/explorer view", "command": "workbench.view.explorer", "type": "command" },
      { "key": "T", "name": "Show active file in tree/explorer view", "command": "workbench.files.action.showActiveFileInExplorer", "type": "command" },
      { "key": "y", "name": "Copy path of active file", "command": "workbench.action.files.copyPathOfActiveFile", "type": "command" },
      { "key": "o", "name": "Open with", "command": "explorer.openWith", "type": "command" },
      { "key": "l", "name": "Change file language", "command": "workbench.action.editor.changeLanguageMode", "type": "command" }
    ]},
    { "key": "g", "name": "+Git", "type": "bindings", "bindings": [
      { "key": "b", "name": "Checkout", "command": "git.checkout", "type": "command" },
      { "key": "c", "name": "Commit", "command": "git.commit", "type": "command" },
      { "key": "d", "name": "Delete Branch", "command": "git.deleteBranch", "type": "command" },
      { "key": "f", "name": "Fetch", "command": "git.fetch", "type": "command" },
      { "key": "i", "name": "Init", "command": "git.init", "type": "command" },
      { "key": "m", "name": "Merge", "command": "git.merge", "type": "command" },
      { "key": "p", "name": "Publish", "command": "git.publish", "type": "command" },
      { "key": "s", "name": "Status", "command": "workbench.view.scm", "type": "command" },
      { "key": "S", "name": "Stage", "command": "git.stage", "type": "command" },
      { "key": "U", "name": "Unstage", "command": "git.unstage", "type": "command" }
    ]},
    { "key": "p", "name": "+Project", "type": "bindings", "bindings": [
      { "key": "f", "name": "Find file in project", "command": "workbench.action.quickOpen", "type": "command" },
      { "key": "p", "name": "Switch project", "command": "workbench.action.openRecent", "type": "command" },
      { "key": "t", "name": "Show tree/explorer view", "command": "workbench.view.explorer", "type": "command" }
    ]},
    { "key": "r", "name": "+Repeat", "type": "bindings", "bindings": [
      { "key": ".", "name": "Repeat recent actions", "command": "whichkey.repeatRecent", "type": "command" }
    ]},
    { "key": "s", "name": "+Search/Symbol", "type": "bindings", "bindings": [
      { "key": "e", "name": "Edit symbol", "command": "editor.action.rename", "type": "command" },
      { "key": "h", "name": "Highlight symbol transient", "commands": [ "editor.action.wordHighlight.trigger", "whichkey.showTransient" ], "args": [ null, "whichkey.transient.symbol" ], "type": "commands" },
      { "key": "j", "name": "Jump to symbol in file", "command": "workbench.action.gotoSymbol", "type": "command" },
      { "key": "J", "name": "Jump to symbol in workspace", "command": "workbench.action.showAllSymbols", "type": "command" },
      { "key": "p", "name": "Search in a project", "command": "workbench.action.findInFiles", "type": "command" },
      { "key": "P", "name": "Search in a project with a selection", "commands": [ "editor.action.addSelectionToNextFindMatch", "workbench.action.findInFiles" ], "type": "commands" },
      { "key": "r", "name": "Search all references", "command": "editor.action.referenceSearch.trigger", "type": "command" },
      { "key": "R", "name": "Search all references in side bar", "command": "references-view.find", "type": "command" },
      { "key": "s", "name": "Search in current file", "command": "actions.find", "type": "command" }
    ]},
    { "key": "S", "name": "+Show", "type": "bindings", "bindings": [
      { "key": "e", "name": "Show explorer", "command": "workbench.view.explorer", "type": "command" },
      { "key": "s", "name": "Show search", "command": "workbench.view.search", "type": "command" },
      { "key": "g", "name": "Show source control", "command": "workbench.view.scm", "type": "command" },
      // { "key": "t", "name": "Show test", "command": "workbench.view.extension.test", "type": "command" },
      { "key": "r", "name": "Show remote explorer", "command": "workbench.view.remote", "type": "command" },
      { "key": "x", "name": "Show extensions", "command": "workbench.view.extensions", "type": "command" },
      { "key": "p", "name": "Show problem", "command": "workbench.actions.view.problems", "type": "command" },
      { "key": "o", "name": "Show output", "command": "workbench.action.output.toggleOutput", "type": "command" }
      // { "key": "d", "name": "Show debug console", "command": "workbench.debug.action.toggleRepl", "type": "command" }
    ]},
    { "key": "t", "name": "+Toggles", "type": "bindings", "bindings": [
      { "key": "b", "name": "Toggle side bar visibility", "command": "workbench.action.toggleSidebarVisibility", "type": "command" },
      // { "key": "c", "name": "Toggle find case sensitive", "command": "toggleFindCaseSensitive", "type": "command" },
      { "key": "j", "name": "Toggle panel visibility", "command": "workbench.action.togglePanel", "type": "command" },
      { "key": "F", "name": "Toggle full screen", "command": "workbench.action.toggleFullScreen", "type": "command" },
      { "key": "s", "name": "Select theme", "command": "workbench.action.selectTheme", "type": "command" },
      { "key": "m", "name": "Toggle maximized panel", "command": "workbench.action.toggleMaximizedPanel", "type": "command" },
      { "key": "t", "name": "Toggle tool/activity bar visibility", "command": "workbench.action.toggleActivityBarVisibility", "type": "command" },
      { "key": "T", "name": "Toggle tab visibility", "command": "workbench.action.toggleTabsVisibility", "type": "command" },
      // { "key": "w", "name": "Toggle ignore trim whitespace in diff", "command": "toggle.diff.ignoreTrimWhitespace", "type": "command" },
      { "key": "w", "name": "Toggle word wrap", "command": "editor.action.toggleWordWrap", "type": "command" },
      { "key": "z", "name": "Toggle zen mode", "command": "workbench.action.toggleZenMode", "type": "command" }
    ]},
    { "key": "w", "name": "+Window", "type": "bindings", "bindings": [
